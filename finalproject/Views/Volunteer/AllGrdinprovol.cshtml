@model IEnumerable<finalproject.Models.Grades>

@{
    ViewBag.Title = "AllGrdinprovol";
}

<h2>AllGrdinprovol</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Grade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pr_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ch_Id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Grade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pr_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ch_Id)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditGrade", new { id=item.Gr_Id }) |
            @Html.ActionLink("Details", "DetailsGrade", new { id=item.Gr_Id }) |
            @Html.ActionLink("Delete", "/") @*, new { id=item.Gr_Id })*@
        </td>
    </tr>
}

</table>
